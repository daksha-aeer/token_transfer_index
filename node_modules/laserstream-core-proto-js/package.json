{
  "name": "laserstream-core-proto-js",
  "version": "9.0.2",
  "description": "Protocol buffer definitions for LaserStream gRPC (Yellowstone-compatible)",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "pbjs -t static-module -w commonjs -o generated.js geyser.proto solana-storage.proto && pbts -o generated.d.ts generated.js",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "generated.js",
    "generated.d.ts",
    "geyser.proto",
    "solana-storage.proto"
  ],
  "keywords": [
    "laserstream",
    "yellowstone",
    "solana",
    "grpc",
    "protobuf",
    "geyser"
  ],
  "author": "Helius <support@helius.xyz>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helius-labs/laserstream.git",
    "directory": "laserstream-core-proto-js"
  },
  "dependencies": {
    "protobufjs": "^7.5.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "protobufjs-cli": "^1.1.3",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
